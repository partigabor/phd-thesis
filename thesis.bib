@article{dakin_cacao_2000,
  title = {Cacao and {{Chocolate}}: {{A Uto-Aztecan Perspective}}},
  shorttitle = {{{CACAO AND CHOCOLATE}}},
  author = {Dakin, Karen and Wichmann, Søren},
