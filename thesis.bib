@article{xiang_chemical_2016,
  title = {The {{Chemical}} and {{Genetic Characteristics}} of {{Szechuan Pepper}} ({{Zanthoxylum}} Bungeanum and {{Z}}. Armatum) {{Cultivars}} and {{Their Suitable Habitat}}},
  author = {Xiang, Li and Liu, Yue and Xie, Caixiang and Li, Xiwen and Yu, Yadong and Ye, Meng and Chen, Shilin},
  date = {2016-04-19},
  journaltitle = {Frontiers in Plant Science},
